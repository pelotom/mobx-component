{
  "name": "mobx-component",
  "version": "0.1.4",
  "description": "Simplified React components via MobX",
  "author": "Tom Crockett <pelotom@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pelotom/mobx-component.git"
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist typings tmp coverage .nyc_output",
    "lint": "tslint --project tsconfig.json --format verbose",
    "test": "nyc ava",
    "upload-coverage": "nyc report --reporter=text-lcov | coveralls",
    "prebuild": "npm run clean",
    "pretest": "tsc",
    "posttest": "npm run lint",
    "preversion": "npm test",
    "postversion": "git push && git push --tags",
    "prepublish": "cross-env NODE_ENV=production npm run build"
  },
  "keywords": [
    "typescript",
    "lib",
    "library",
    "boilerplate"
  ],
  "files": [
    "index.d.ts"
  ],
  "engines": {
    "node": ">=4.6.0",
    "npm": ">=2.15.9"
  },
  "peerDependencies": {
    "mobx": "<3",
    "react": "<16"
  },
  "devDependencies": {
    "ava": "^0.16.0",
    "change-case": "^3.0.0",
    "coveralls": "^2.11.11",
    "cross-env": "^3.0.0",
    "mobx": "^2.6.0",
    "mobx-react": "^3.5.8",
    "nyc": "^8.0.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "rimraf": "^2.5.3",
    "tslint": "^3.15.1",
    "typescript": "^2.0.3"
  },
  "dependencies": {
    "@types/react": "^0.14.41"
  }
}
